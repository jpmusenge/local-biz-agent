{
  "name": "local-biz-agent",
  "version": "1.0.0",
  "description": "AI agent that discovers new businesses and generates websites for them",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "discover": "tsx scripts/discover.ts",
    "enrich": "tsx scripts/enrich.ts",
    "generate": "tsx scripts/generate.ts",
    "deploy": "tsx scripts/deploy.ts",
    "pipeline": "tsx scripts/pipeline.ts",
    "pipeline:discover": "tsx scripts/pipeline.ts --discover-only",
    "pipeline:generate": "tsx scripts/pipeline.ts --generate-only",
    "pipeline:deploy": "tsx scripts/pipeline.ts --deploy-only",
    "pipeline:dry-run": "tsx scripts/pipeline.ts --dry-run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test:db": "tsx scripts/test-db.ts",
    "preview": "tsx scripts/preview.ts",
    "test:premium": "tsx scripts/test-premium.ts",
    "test:restaurant-premium": "tsx scripts/test-restaurant-premium.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "business",
    "website-generator",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.0",
    "better-sqlite3": "^9.2.0",
    "dotenv": "^16.3.0",
    "ts-node": "^10.9.0"
  }
}
