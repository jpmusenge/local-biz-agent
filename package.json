{
  "name": "local-biz-agent",
  "version": "1.0.0",
  "description": "AI agent that discovers new businesses and generates websites for them",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "discover": "ts-node --esm scripts/discover.ts",
    "enrich": "ts-node --esm scripts/enrich.ts",
    "generate": "ts-node --esm scripts/generate.ts",
    "deploy": "ts-node --esm scripts/deploy.ts",
    "pipeline": "ts-node --esm scripts/pipeline.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "agent",
    "business",
    "website-generator",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^9.2.0",
    "dotenv": "^16.3.0",
    "ts-node": "^10.9.0"
  }
}
